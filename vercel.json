{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/chat-support.js",
      "dest": "/public/chat-support.js"
    },
    {
      "src": "/(.*\\.js)$",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*\\.css)$",
      "dest": "/public/$1"
    },
    {
      "src": "/index.html",
      "dest": "/server.js"
    },
    {
      "src": "/login.html",
      "dest": "/server.js"
    },
    {
      "src": "/register.html",
      "dest": "/server.js"
    },
    {
      "src": "/authenticator.html",
      "dest": "/server.js"
    },
    {
      "src": "/reports.html",
      "dest": "/server.js"
    },
    {
      "src": "/orders.html",
      "dest": "/server.js"
    },
    {
      "src": "/check.html",
      "dest": "/server.js"
    },
    {
      "src": "/scammed-reports.html",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
