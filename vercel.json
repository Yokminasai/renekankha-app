{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/index.html",
      "dest": "/server.js"
    },
    {
      "src": "/login.html",
      "dest": "/server.js"
    },
    {
      "src": "/register.html",
      "dest": "/server.js"
    },
    {
      "src": "/authenticator.html",
      "dest": "/server.js"
    },
    {
      "src": "/reports.html",
      "dest": "/server.js"
    },
    {
      "src": "/orders.html",
      "dest": "/server.js"
    },
    {
      "src": "/check.html",
      "dest": "/server.js"
    },
    {
      "src": "/scammed-reports.html",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
